{
  "name": "c0rbot",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": ["dbaeumer.vscode-eslint", "esbenp.prettier-vscode"]
    }
  },
  "postCreateCommand": "yarn install",
  "remoteUser": "node",
  "mounts": [
    "source=${env:HOME}${env:USERPROFILE}/.aws,target=/home/node/.aws,type=bind"
  ]
}
